{
  "name": "ai-photo-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "cross-env NODE_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "playwright test",
    "test:e2e:admin": "playwright test tests/e2e/admin/**/*.spec.ts",
    "test:admin:all": "chmod +x scripts/run-admin-tests.sh && ./scripts/run-admin-tests.sh",
    "test:admin:user": "./scripts/run-admin-tests.sh user-management",
    "test:admin:pipeline": "./scripts/run-admin-tests.sh pipeline-management",
    "test:admin:kb": "./scripts/run-admin-tests.sh knowledge-base",
    "test:admin:config": "./scripts/run-admin-tests.sh system-config",
    "test:admin:integration": "./scripts/run-admin-tests.sh integration",
    "test:admin:ci": "./scripts/run-admin-tests.sh --ci --headless",
    "test:admin:debug": "PWDEBUG=true ./scripts/run-admin-tests.sh --debug",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "a11y:check": "node scripts/lighthouse-a11y.js",
    "a11y:install": "npm install --save-dev lighthouse chrome-launcher",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:serve": "npx http-server storybook-static",
    "security:check": "node scripts/security-check.js",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:depcheck": "npx depcheck",
    "rollback:version": "node scripts/rollback-version.js",
    "rollback:dry-run": "node scripts/rollback-version.js --dry-run"
  },
