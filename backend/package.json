{
  "name": "ai-photo-backend",
  "version": "1.0.0",
  "description": "服装AI处理 SaaS 平台后端服务",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:test:migrate": "NODE_ENV=test knex migrate:latest",
    "db:test:rollback": "NODE_ENV=test knex migrate:rollback",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "saas",
    "fashion",
    "image-processing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "alipay-sdk": "^4.14.0",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "morgan": "^1.10.1",
    "mysql2": "^3.6.5",
    "nanoid": "^3.3.11",
    "node-cron": "^3.0.3",
    "qcloud-cos-sts": "^3.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tencentcloud-sdk-nodejs": "^4.0.674",
    "wechatpay-node-v3": "^2.2.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^8.55.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.0.0"
  }
