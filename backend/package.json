{
  "name": "ai-photo-backend",
  "version": "1.0.0",
  "description": "服装AI处理 SaaS 平台后端服务",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch --preserveWatchOutput",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.ts",
    "test:unit": "jest --config jest.unit.config.ts",
    "test:unit:coverage": "jest --config jest.unit.config.ts --coverage",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:test:migrate": "NODE_ENV=test knex migrate:latest",
    "db:test:rollback": "NODE_ENV=test knex migrate:rollback",
    "lint": "eslint src --ext .ts --max-warnings=0",
    "format": "prettier --write \"{src,tests,scripts}/**/*.{ts,tsx,js,json}\""
  },
  "keywords": [
    "ai",
    "saas",
    "fashion",
    "image-processing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@hapi/joi": "^17.1.1",
    "alipay-sdk": "^4.14.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.63.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsdoc": "^4.0.5",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lru-cache": "^11.2.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "nanoid": "^5.1.6",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "p-limit": "^7.2.0",
    "prom-client": "^15.1.3",
    "qcloud-cos-sts": "^3.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tencentcloud-sdk-nodejs": "^4.1.137",
    "uuid": "^13.0.0",
    "wechat-api": "^1.35.1",
    "wechatpay-node-v3": "^2.2.1",
    "winston": "^3.18.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lru-cache": "^7.10.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.16.11",
    "@types/swagger-ui-express": "^4.1.8",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  }
}
