{
  "name": "ai-photo-backend",
  "version": "1.0.0",
  "description": "服装AI处理 SaaS 平台后端服务",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --config jest.unit.config.js",
    "test:unit:coverage": "jest --config jest.unit.config.js --coverage",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:test:migrate": "NODE_ENV=test knex migrate:latest",
    "db:test:rollback": "NODE_ENV=test knex migrate:rollback",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "saas",
    "fashion",
    "image-processing"
  ],
  "author": "",
  "license": "ISC",
